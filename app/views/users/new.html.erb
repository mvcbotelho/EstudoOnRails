<h1>Cadastro</h1>
		
	<%= form_for @users do |f| %>
		<p>
			<%= f.label :full_name %><br />
			<%= f.text_field :full_name %>
		</p>
			
		<p>
			<%= f.label :location %><br />
			<%= f.text_field :location %>
		</p>
		
		<p>
			<%= f.label :email %><br />
			<%= f.text_field :email %>
		</p>
			
		<p>
			<%= f.label :password %><br />
			<%= f.text_field :password %>
		</p>
			
		<p>
			<%= f.label :password_confirmation %><br />
			<%= f.text_field :password_confirmation %>
		</p>
			
		<p>
			<%= f.label :bio %><br />
			<%= f.text_field :bio %>
		</p>
			
		<p>
			<%= f.submit %>
		</p>
			
		<% end %>